<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>Exe</OutputType>
    <TargetFramework>net472</TargetFramework>
    <LangVersion>10</LangVersion>
    <Nullable>enable</Nullable>
    <Version>1.2.0</Version>
    <Company>AufBauWerk - Unternehmen für junge Menschen</Company>
    <Copyright>Copyright © 2015-2022 Manuel Meitinger</Copyright>
    <Description>Syncs extension states across multiple Asterisk servers and sends SMS messages on missed calls.</Description>
    <Product>AufBauWerk Asterisk Manager</Product>
    <RootNamespace>Aufbauwerk.Asterisk</RootNamespace>
